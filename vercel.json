{
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/api/:path*",
      "dest": "https://ssw.inf.br/api/:path*"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}